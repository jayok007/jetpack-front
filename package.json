{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "concurrently \"npm:vue:start\" \"npm:mb:start\"",
    "vue:start": "vue-cli-service serve",
    "mb:start": "mb  --configfile=tests/mock/imposters.json --allowInjection --nologfile",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:e2e": "concurrently \"vue-cli-service test:e2e\" \"npm:mb:start\"",
    "test:e2e:ci": "vue-cli-service test:e2e --headless",
    "test:unit": "vue-cli-service test:unit --coverage"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "material-design-icons-iconfont": "^3.0.3",
    "roboto-fontface": "*",
    "vue": "^2.6.6",
    "vue-router": "^3.0.1",
    "vuetify": "^1.5.5"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^1.2.0",
    "@stryker-mutator/html-reporter": "^1.2.0",
    "@stryker-mutator/javascript-mutator": "^1.2.0",
    "@stryker-mutator/jest-runner": "^1.2.0",
    "@stryker-mutator/vue-mutator": "^1.2.0",
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-e2e-cypress": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-plugin-unit-jest": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "concurrently": "^4.1.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "lint-staged": "^8.1.4",
    "mountebank": "^2.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-cli-plugin-vuetify": "^0.5.0",
    "vue-template-compiler": "^2.5.21",
    "vuetify-loader": "^1.0.5"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "vue-cli-service test:unit --findRelatedTests",
      "git add"
    ]
  }
}
